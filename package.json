{
  "name": "portfolio-ng",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"*.{json,md}\" \"{src,e2e}/**/*.{js,ts,json,html}\"",
    "e2e": "ng e2e",
    "now-build": "yarn build --optimization=true --prod=true --outputHashing=all && mv dist/portfolio-ng . && rm -r dist && mv portfolio-ng dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.0.0",
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/forms": "8.0.0",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@angular/router": "8.0.0",
    "@angular/service-worker": "8.0.0",
    "core-js": "3.1.4",
    "rxjs": "6.5.2",
    "tslib": "1.9.3",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.800.0",
    "@angular/cli": "8.0.0",
    "@angular/compiler-cli": "8.0.0",
    "@angular/language-service": "8.0.0",
    "@types/jasmine": "3.3.13",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.0.3",
    "codelyzer": "5.0.1",
    "husky": "2.3.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "lint-staged": "8.1.7",
    "prettier": "1.17.1",
    "protractor": "6.0.0",
    "ts-node": "8.2.0",
    "tslint": "5.16.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "^3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test --browsers ChromeHeadless --watch=false && lint-staged"
    }
  }
}
